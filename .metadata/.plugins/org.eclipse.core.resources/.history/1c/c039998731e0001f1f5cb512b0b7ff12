package tascaS102Ex1;

import java.util.*;

class Product {
	private String name;
	private double price;

	public Product(String name, double price) {
		this.name = name;
		this.price = price;
	}

	public double getPrice() {
		return price;
	}

	public String getName() {
		return name;
	}
}

class SaleBuidedException extends Exception {
	public SaleBuidedException(String message) {
		super(message);
	}
}
